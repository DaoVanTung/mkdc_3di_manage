{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "mkdc_3di_manage",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "mkdc_3di_manage (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "mkdc_3di_manage (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "mkdc_3di_manage web",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-renderer",
                "html",
                "--web-hostname",
                "localhost",
                "-d",
                "chrome",
                "--web-port",
                "5000",
                "--web-browser-flag",
                "--disable-web-security"
            ],
            "program": "lib/main.dart"
        },
    ]
}